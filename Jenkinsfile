pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
   
        stage('Build') {
            steps {
                echo 'Project Building'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Project Deploying'
            }
        }
        stage('Test') {
            steps {
                echo 'Project Testing'
            }
        }
        stage('Release') {
            steps {
                echo 'Project Release'
            }
        }
    }
}
